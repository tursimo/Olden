{
	"manifest_version": 2,
	"name": "Olden",
	"version": "1.0",
	"content_scripts": [
		{
			"run_at": "document_start",
			"exclude_matches": [
				"*://old.reddit.com/*"
			],
			"matches": [
				"*://*.reddit.com/*"
			],
			"js": [
				"olden.js"
			]
		}
	],
	"browser_specific_settings": {
		"gecko": {
			"id": "id@placeholder.com"
		}
	}
}